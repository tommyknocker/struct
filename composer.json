{
  "name": "tommyknocker/struct",
  "description": "PHP Structure helper with typed properties and attributes",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "tommyknocker\\struct\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tommyknocker\\struct\\tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
  }
}
